{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@styles/*": ["styles/*"],
      "@root/*": ["./*"],
      "@root": ["./"],
      "@atoms": ["components/atoms/"],
      "@atoms/*": ["components/atoms/*"],
      "@pages": ["components/pages/"],
      "@pages/*": ["components/pages/*"],
      "@custom-antd": ["components/custom-antd"],
      "@custom-antd/*": ["components/custom-antd/*"],
      "@molecules": ["components/molecules/"],
      "@molecules/*": ["components/molecules/*"],
      "@organisms": ["components/organisms/"],
      "@organisms/*": ["components/organisms/*"],
      "@antd-theme": ["antd-theme/"],
      "@antd-theme/*": ["antd-theme/*"],
      "@assets": ["assets/images/"],
      "@assets/*": ["assets/images/*"],
      "@contexts": ["contexts/"],
      "@contexts/*": ["contexts/*"],
      "@utils": ["utils/"],
      "@models": ["models/"],
      "@models/*": ["models/*"],
      "@utils/*": ["utils/*"],
      "@types": ["types/"],
      "@types/*": ["types/*"],
      "@constants": ["constants/"],
      "@constants/*": ["constants/*"],
      "@hooks": ["hooks/"],
      "@hooks/*": ["hooks/*"],
      "@features": ["features/"],
      "@features/*": ["features/*"],
      "@redux": ["redux/"],
      "@redux/*": ["redux/*"]
